{
  "name": "Socrative Collab",
  "version": "1.0",
  "description": "Build an Extension with Angular",
  "manifest_version": 2,
  "permissions": ["activeTab", "webNavigation", "storage", "tabs"],
  "background": {
    "scripts": ["background-es2015.js", "runtime-es2015.js"]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": ["index.html"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content-es2015.js"]
    }
  ]
}
